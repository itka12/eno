# Subject: while statment

# without using loop statement
print(1)
print(2)
print(3)
print(4)
print(5)
print(6)
print(7)
print(8)
print(9)
print(10)

# print numbers from 1 to 10 with while
loop = 1
while loop <= 10:
    print(loop)
    loop += 1 # same as loop = loop + 1
  
